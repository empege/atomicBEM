<section class="m-slider js-slider">
  <button class="m-slider__arrow m-slider__arrow--left js-arrow-left
    "> {{!-- Ovaj path nema smisla, radice samo ako se koristi u faq. Ne znam kako da namestim relative path da radi u handlebars, samo absolute radi ili relative od fajla u kom se ovo zapravo koristi i u kom se poziva --}}<img src="images/arrowleft.png" alt="arrow-left" class="js-slide-arrow" /> </button>
  <button class="m-slider__arrow m-slider__arrow--right js-arrow-right"> <img src="images/arrowright.png" alt="arrow-right" class="js-slide-arrow" /> </button>
  <div class="m-slider__inner js-slider-inner">
    {{#each data}}
    <a draggable="false" class="m-slider__slide js-slide" target="_blanc" href={{url}}>
      <img draggable="false" class="m-slider__image" src={{img}} alt="slide-image">
      <div class="m-slider__content">
        <h3 class="m-slider__title">{{title}}</h3>
        <p class="m-slider__description">{{text}}</p>
        <div class="m-slider__action-btn">
          <span class="read-more">Read More</span>
        </div>
      </div>
    </a>
    {{/each}}
